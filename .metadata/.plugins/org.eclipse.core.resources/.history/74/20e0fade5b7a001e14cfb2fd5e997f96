
public class Ejer22 {

	public static void main(String[] args) {
		// Escribe una aplicaci√≥n que muestre las tablas de multiplicar
		// del 1 al 10

		for (int i = 1; i <= 10; i++) {
			System.out.println();
			for (int j = 1; j <= 10; j++) {
				System.out.println(i + " * " + j + " = " + i * j);
				
			}
		}

	}

}
