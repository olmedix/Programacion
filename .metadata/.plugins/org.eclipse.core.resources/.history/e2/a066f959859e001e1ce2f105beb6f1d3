package hoja3;

public class Ejer06 {

	public static void main(String[] args) {
		
		String[][] tabla = new String[3][2];
		
		tabla[0][0]= "pan";
		tabla[0][1]= "0.75";
		tabla[1][0]= "leche";
		tabla[1][1]= "0.95";
		tabla[2][0]= "agua";
		tabla[2][1]= "1.25";
		
		System.out.println("Producto\tPrecio");
		for (int i = 0; i < tabla.length; i++) {
			for (int j = 0; j < tabla[i].length; j++) {
				System.out.print(tabla[i][j] + "\t");
			}
			System.out.println();
		}
	
	}

}
