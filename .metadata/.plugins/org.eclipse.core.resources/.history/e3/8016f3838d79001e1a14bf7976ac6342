package hoja2Bucles;

public class Ejer15 {

	public static void main(String[] args) {
		// Crea una aplicaci√≥n que calcule una lista con las potencias de 3 menores que 12000. 
		//Haz que en cada pasada del bucle se muestre el exponente y el valor de la potencia. 
		//Utiliza un bucle do-while
		int resultado=0;
		do {
			int exponente =1;
			int potencia=3;
			resultado = exponente * potencia;
			System.out.println(exponente + "^"+ potencia + ": " + resultado );
			exponente++;
		}while(resultado<1200);
	}

}
